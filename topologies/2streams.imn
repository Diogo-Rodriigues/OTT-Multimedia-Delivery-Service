    type router
    model host
    network-config {
	hostname n3
	!
	interface eth0
	 ip address 10.0.0.10/24
	 ipv6 address 2001:0::10/64
	!
    }
    canvas c1
    iconcoords {681.0 206.0}
    labelcoords {681.0 238.0}
    interface-peer {eth0 n7}
